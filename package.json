{
  "name": "market-maker",
  "version": "1.0.0",
  "description": "market maker for LEV via Getcoin",
  "main": "index.js",
  "scripts": {
    "start": "node lib/index.js",
    "start-babelling": "babel-node src/index.js",
    "test": "jest --watch",
    "prepublish": "npm run clean && npm run build",
    "clean": "rimraf lib",
    "version:patch": "npm version patch",
    "build": "babel src --out-dir lib --source-maps",
    "build:watch": "babel src --out-dir lib --source-maps --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leverj/market-maker/.git"
  },
  "keywords": [
    "leverj",
    "LEV",
    "trading"
  ],
  "author": "benny@leverj.io",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leverj/market-maker/issues"
  },
  "homepage": "https://github.com/leverj/market-maker/#readme",
  "jest": {
    "testMatch": [
      "**/src/**/?(*.)(spec|test|it).js"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-node8": "^1.2.0",
    "jest": "^21.2.1",
    "jest-immutable-matchers": "^1.5.0",
    "pm2": "^2.8.0"
  },
  "dependencies": {
    "babel-plugin-source-map-support": "^1.0.0",
    "babel-polyfill": "^6.26.0",
    "config": "^1.28.1",
    "crypto-js": "^3.1.9-1",
    "decimal.js": "^7.4.0",
    "fetchival": "^0.3.3",
    "immutable": "^3.8.2",
    "js-yaml": "^3.10.0",
    "log4js": "^2.3.12",
    "node-fetch": "^1.7.3",
    "pubnub": "^4.18.0",
    "queue": "^4.4.2",
    "slack-node": "^0.1.8",
    "source-map-support": "^0.5.0"
  }
}
